<link rel="stylesheet" href="{{ 'component-list-social.css' | asset_url }}" media="print" onload="this.media='all'">


{% assign twitter_link = "https://twitter.com/CookieDietAus" %}
{% assign facebook_link = "https://www.facebook.com/CookieDietAustralia" %}
{% assign instagram_link = "https://www.instagram.com/cookie.diet/" %}
{% assign youtube_link = "https://www.youtube.com/channel/UCoaoyYdPy-41fIZjmNIeWmg/videos" %}


  <ul class="list-unstyled list-socials" role="list">
    {%- if twitter_link != blank -%}
      <li class="list-social__item">
        <a href="{{ twitter_link }}" class="link list-social__link" >
          {%- render 'icon-twitter' -%}
          <span class="visually-hidden">{{ 'general.social.links.twitter' | t }}</span>
        </a>
      </li>
    {%- endif -%}
    {%- if facebook_link != blank -%}
      <li class="list-social__item">
